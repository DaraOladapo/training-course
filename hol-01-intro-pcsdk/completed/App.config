<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1" />
  </startup>
  <appSettings>
    <!-- azure ad tenant details -->
    <add key="aad:tenant-id" value=""/>

    <!-- azure ad application details -->
    <add key="aad:client-id" value=""/>
    <add key="aad:client-secret" value=""/>

    <!-- partner center api -->
    <add key="partnercenter-endpoint" value="https://api.partnercenter.microsoft.com"/>
    <add key="partnercenter-resource" value="https://api.partnercenter.microsoft.com"/>

    <!-- user details -->
    <add key="partner-user" value=""/>
